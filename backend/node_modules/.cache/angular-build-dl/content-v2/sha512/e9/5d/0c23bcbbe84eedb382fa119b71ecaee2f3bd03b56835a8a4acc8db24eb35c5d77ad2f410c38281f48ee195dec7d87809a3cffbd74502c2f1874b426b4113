class InvoicesComponent {
    constructor(route, draftInvoicesStoreService, invoiceWebService, translateService) {
        this.route = route;
        this.draftInvoicesStoreService = draftInvoicesStoreService;
        this.invoiceWebService = invoiceWebService;
        this.translateService = translateService;
        this.subs = new src_app_shared_services_subscription_manager__WEBPACK_IMPORTED_MODULE_0__.SubscriptionManager();
    }
    ngOnInit() {
        this.subs.sink = this.invoiceWebService
            .getEntities()
            .subscribe((invoices) => {
            if (invoices) {
                this.invoices = this.mapDataToTableShow(invoices);
            }
        });
    }
    mapDataToTableShow(invoices) {
        let table = {
            header: [
                this.translateService.instant('code'),
                this.translateService.instant('date'),
                this.translateService.instant('advancePayment'),
                this.translateService.instant('buyerName'),
            ],
            data: [],
        };
        invoices.forEach((entity) => {
            table.data.push(entity.oid);
            table.data.push(entity.createDate);
            table.data.push(entity.advancePayment);
            table.data.push(entity.buyerName);
        });
        return table;
    }
    editInvoice(oid) {
        this.route.navigate(['invoice-create-edit', 'edit', oid]);
    }
    cancel() {
        this.route.navigate(['/']);
    }
    clear() {
        this.draftInvoicesStoreService.clearDraftInvoices();
    }
    ngOnDestroy() {
        this.subs.unsubscribe();
    }
}
