class EntityBaseWebService {
    constructor(baseWebService, domainName) {
        this.baseWebService = baseWebService;
        this.domainName = domainName;
        this.URL_PREFIX = 'http://localhost:45328/';
    }
    getEntities() {
        return this.baseWebService.getRequestForArray(this.URL_PREFIX + this.domainName);
    }
    getEntityByOid(oid) {
        return this.baseWebService.getRequest(this.URL_PREFIX + this.domainName + '/' + oid);
    }
    createEntity(data) {
        return this.baseWebService.postRequest(this.URL_PREFIX + this.domainName, data);
    }
    updateEntity(data) {
        return this.baseWebService.putRequest(this.URL_PREFIX + this.domainName, data);
    }
    deleteEntity(data) {
        console.log(data);
        return this.baseWebService.deleteRequest(this.URL_PREFIX + this.domainName);
    }
}
